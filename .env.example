# Docker Versions
CHVER=22.8.20.11-alpine
CHKVER=22.8.20.11
PGVER=17.0

# PostgreSQL (Trading Database)
PG_USER=your_postgres_user
PG_PASSWORD=your_secure_password
PG_DATABASE=pthm_local
PG_HOST=localhost
PG_PORT=5435
PG_POOL_SIZE=10

# PostgreSQL (Auth Database)
PG_AUTH_USER=your_postgres_user
PG_AUTH_PASSWORD=your_secure_password
PG_AUTH_DATABASE=pthm_auth
PG_AUTH_HOST=localhost
PG_AUTH_PORT=5435

# PostgreSQL (Token Exchange Database)
PG_TOKEN_USER=your_postgres_user
PG_TOKEN_PASSWORD=your_secure_password
PG_TOKEN_DATABASE=pthm_token
PG_TOKEN_HOST=localhost
PG_TOKEN_PORT=5435

# ByBit API Configuration
# Get your API keys from: https://www.bybit.com/app/user/api-management
BYBIT_API_KEY=your_bybit_api_key
BYBIT_API_SECRET=your_bybit_api_secret
BYBIT_API_URL=https://api-testnet.bybit.com

# ClickHouse Configuration
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_DB=trading_app
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=8125

# SSL/TLS Keystore Configuration
# Paths to your Java keystores for mTLS authentication
KEY_STORE_PATH=/path/to/your/keystore.jks
KEY_STORE_PASSWORD=your_keystore_password
KEY_PASSWORD=your_key_password
TRUST_STORE_PATH=/path/to/your/truststore.jks
TRUSTSTORE_PASSWORD=your_truststore_password

# WebSocket Server Keystore Configuration
WS_KEYSTORE_PATH=/path/to/your/websocket/keystore.jks
WS_KEYSTORE_PASSWORD=your_websocket_keystore_password

# Production/Jenkins Configuration
PROD_CLICKHOUSE_HOST=your_production_clickhouse_ip
